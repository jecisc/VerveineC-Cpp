			<groupId>org.eclipse.ui</groupId>
			<artifactId>org.eclipse.ui</artifactId>
			<version>3.3.0</version>
		</dependency>
		<!-- https://mvnrepository.com/artifact/org.eclipse.core/org.eclipse.core.jobs -->
		<dependency>
			<groupId>org.eclipse.core</groupId>
			<artifactId>org.eclipse.core.jobs</artifactId>
			<version>3.5.300.v20130429-1813</version>
		</dependency>

		<!-- local dependencies -->
		<dependency>
			<groupId>akuhn</groupId>
			<artifactId>util</artifactId>
			<version>1</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/akuhn-util-r28011.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>fame</groupId>
			<artifactId>core</artifactId>
			<version>1</version>
